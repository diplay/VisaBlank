  <fieldset>
    <div class="form-group" >
      <%= f.label :fio, "ФИО", class: "control-label col-sm-2" %>
      <div class="col-sm-10" >
        <%= f.text_field :fio, class: "form-control", required: true %>
      </div>
    </div>
    <div class="form-group" >
      <%= f.label :date_of_birth, "Дата рождения",
        class: "control-label col-sm-2"%>
      <div class="col-sm-10" >
        <%= f.date_field :date_of_birth, class: "form-control" %>
      </div>
    </div>
    <div class="form-group" >
      <%= f.label :sex, "Пол", class: "control-label col-sm-2" %>
      <div class="col-sm-10" >
        <label class="radio-inline" >
          <%= f.radio_button :sex, :true %>
          Мужской
        </label>
        <label class="radio-inline" >
          <%= f.radio_button :sex, :false %>
          Женский
        </label>
      </div>
    </div>
    <h3>Паспорт</h3>
    <div class="form-group" >
      <%= f.label :passport_number_series, "Серия",
        class: "control-label col-sm-1"%>
      <div class="col-sm-1" >
        <%= f.text_field :passport_number_series, class: "form-control input-sm" %>
      </div>
      <%= f.label :passport_number, "Номер",
        class: "control-label col-sm-1" %>
      <div class="col-sm-2" >
        <%= f.text_field :passport_number, class: "form-control input-sm" %>
      </div>
      <%= f.label :passport_given_date, "Дата",
        class: "control-label col-sm-1" %>
      <div class="col-sm-2" >
        <%= f.date_field :passport_given_date, class: "form-control input-sm" %>
      </div>
      <%= f.label :passport_given_code, "Код подразделения",
        class: "control-label col-sm-2" %>
      <div class="col-sm-2" >
        <%= f.text_field :passport_given_code, class: "form-control input-sm" %>
      </div>
    </div>
    <div class="form-group" >
      <%= f.label :passport_given, "Выдан",
        class: "control-label col-sm-1"%>
      <div class="col-sm-11" >
          <%= f.text_field :passport_given, class: "form-control" %>
      </div>
    </div>
    <div class="form-group" >
      <%= f.label :address, "Адрес",
        class: "control-label col-sm-1"%>
      <div class="col-sm-11" >
          <%= f.text_field :address, class: "form-control" %>
      </div>
    </div>
    <div class="form-group" >
      <%= f.label :phone, "Телефон",
        class: "control-label col-sm-1"%>
      <div class="col-sm-11" >
          <%= f.text_field :phone, class: "form-control" %>
      </div>
    </div>
  </fieldset>
  </div>
  <div class="tab-pane fade" id="foreign" >
